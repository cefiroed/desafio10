{{#if data.length }}

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th scope="col">Product</th>
            <th scope="col">Price</th>
            <th scope="col">URL</th>
        </tr>
    </thead>
    <tbody>
        <div>
            {{#each data}}
            <tr>
                <td scope="row">{{this.product}}</td>
                <td scope="row">{{this.price}}</td>
                <td scope="row"><img style="width: 100px" src="{{this.url}}"></img></td>
            </tr>
            {{/each}}
        </div>
    </tbody>
</table>

{{else}}
<p>There are no products</p>
{{/if}} 


